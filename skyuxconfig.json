{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-grids",
  "mode": "easy",
  "compileMode": "aot",
  "host": {
    "url": "https://developer.blackbaud.com"
  },
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "librarySettings": {
    "whitelistedNonPeerDependencies": [
      "dragula",
      "ng2-dragula"
    ]
  },
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "app": {
    "styles": [
      "@skyux/theme/css/sky.css"
    ]
  }
}
